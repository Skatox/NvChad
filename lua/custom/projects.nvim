autocmd VimEnter * if argc() == 0 && !exists("s:std_in") | Welcome | endif

let g:project_enable_welcome = 1

set rtp+=~/.config/nvim/plugged/vim-project

call project#rc("/var/www/")
   Project 'autorepeat'
   Project '/var/www/pricetool/', 'pricetool'
   Project 'startuping'
   Project 'zohotool'

   Project 'wordpress/wp-content/plugins/jquery-archive-list-widget', 'jQuery Archive List'
